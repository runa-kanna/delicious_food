<h1>投稿一覧</h1>

<% @foods.each do |food| %>
<div>
  <%= link_to food_path(food.id) do %>
    <%= image_tag food.get_image %>
  <% end %>
  <p>投稿ユーザー画像:<%= image_tag 'no_image.jpg' %></p>
  <p>タイトル:<%= food.title %></p>
  <p>説明:<%= food.body %></p>
  <p>ユーザーネーム:<%= food.user.name %></p>
</div>
<% end %>